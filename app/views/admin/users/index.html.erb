<h2>ユーザー一覧</h2>

<div class="mt-3">
  <% @users.each do |user| %>
    <div>
      <div>ユーザー名</div>
      <%= user.name %>
      <div>メールアドレス</div>
      <div><%= user.email %></div>
      <div>プロフィール</div>
      <p><%= user.profile %></p>
      <%= link_to "退会", admin_user_path(user), method: :delete, data: {confirm: "このユーザーを退会させますか？"} %>
    </div>
  <% end %>
</div>